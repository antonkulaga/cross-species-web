{
  "name": "cross-species-web",
  "version": "1.0.1",
  "description": "full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.ts",
  "scripts": {
    "try": "ts-node src/server/graph.ts",
    "request": "ts-node src/server/tests/request.ts",
    "build": "webpack --mode production",
    "start": "yarn run build && ts-node src/server/index.ts",
    "dev-start": "yarn run client && ts-node src/server/index.ts",
    "client": "webpack serve --mode development --devtool inline-source-map --hot",
    "server": "nodemon --watch './**/*.ts' --exec 'ts-node' src/server/index.ts",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "test": "jest"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
